{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://host.docker.internal:2810/" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "DeliveryPlatform.WebApi"
    }
  },
  "ConnectionStrings": {
    "Database": "Server=host.docker.internal; port=3307;Database=Delivery;Uid=root;Pwd=admin;",
    "Redis": "host.docker.internal"
  }
}
